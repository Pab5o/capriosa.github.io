{
  "source": "src",
  "destination": "build",
  "plugins": {
    "metalsmith-clean" : true,
    "metalsmith-drafts" : true,
    "contentful-metalsmith": {
      "accessToken" : "40e963dfce9e6bc216d6624cd39c0be5a32bd89e29eee07a1e49a8d30b51bfd6"
    },
    "metalsmith-templates" : {
      "engine" : "handlebars",
      "directory" : "templates"
    },
    "metalsmith-sass" : {
      "includePaths" : ["bower_components/bootstrap-sass-official/assets/stylesheets/"]
    },
    "metalsmith-assets" : {
      "source" : "assets/",
      "destination" : "assets/"
    }
  }
}
